.nav { width: 100%; }
.nav-div { background-color: #263238; }
#menu-menu-1 {
	margin: 0;
	padding: 0;
	list-style: none;
	width: 100%;
	text-align: left;
	min-width:150px;
	font-size:1.1em;
	z-index:20;
}
#menu-menu-1 ul {
	margin: 0;
	padding: 0;
	list-style: none;
	position: absolute;
	top: -999em;
	left: -999em;
	z-index: 2;
	min-width:100%;
	background-color: #263238;
	border-bottom-left-radius:6px;
	border-bottom-right-radius:6px;
	z-index:20;
}
#menu-menu-1 li {
	display: inline-block;
	position: relative;
	text-align: left;
	text-transform:capitalize;
	z-index:20;
	
}
#menu-menu-1 li.focus > ul, 
#menu-menu-1 li:hover > ul {
	top: auto;
	left: auto;	
}
#menu-menu-1 li li {
	display: block;	
}
#menu-menu-1 li li.focus > ul, 
#menu-menu-1 li li:hover > ul {
	left: 100%;
	top: 0;
}
#menu-menu-1 a {
	font-weight:400;  
	color: #eee;
	text-decoration: none;
	display: block;
	white-space: nowrap;
	padding: 8px 10px;
	border-radius:0px;
}


#menu-menu-1 ul li:last-child > a {
	border-bottom-left-radius:6px;	
	border-bottom-right-radius:6px;	
}
#menu-menu-1 ul li.has-children:last-child > a {
	border-bottom-left-radius:6px;	
	border-bottom-right-radius:0px;	
}
#menu-menu-1 ul li ul {
	border-top-right-radius:6px;
}
#menu-menu-1 ul li ul li:first-child > a {
	border-top-right-radius:6px;
}


#menu-menu-1 a,
#menu-menu-1 li.has-children > a:after {
	color: #eee;
}

#menu-menu-1 a:hover,
#menu-menu-1 li:hover > a,
#menu-menu-1 li.has-children > a:hover:after,
#menu-menu-1 li.has-children:hover > a:after {
	color:#fff;
	background:#0073aa;
}

#menu-menu-1 li.has-children > a:after {
	font-family: "Font Awesome 5 Free"!important;
    content: "  \f0d7";
	font-weight:900;
	font-size: 14px;
	vertical-align:bottom;
	white-space: pre;
}

#menu-menu-1 li ul li.has-children > a:before {
	font-family: "Font Awesome 5 Free"!important;
    content: " \f0da";
	font-weight:900;
	font-size: 14px;
	float:right;
}

#menu-menu-1 li ul li.has-children > a:after {
    content: "";
}

#menu-menu-1 .focus > a,
#menu-menu-1 .current-menu-item > a,
#menu-menu-1 .current-menu-ancestor > a,
#menu-menu-1 .current-menu-ancestor > a:after,
#menu-menu-1 .current-post-ancestor > a {
	font-weight:600;
	color: #fff!important;
	background:#0073aa;
}

ul.sub-menu {
	-webkit-box-shadow: 0px 15px 25px 0px rgba(0,0,0,0.3);
	-moz-box-shadow: 0px 15px 25px 0px rgba(0,0,0,0.3);
	box-shadow: 0px 15px 25px 0px rgba(0,0,0,0.3);
}

ul.sub-menu li { font-size:0.9rem;}
ul.sub-menu li.submenu-header:after {
	content:""; 
	background:#4f5b62;
	position:absolute;
	margin-left: auto;
	margin-right: auto;
	left: 0;
	right: 0;
	height: 1px; 
	width: 90%;
}

