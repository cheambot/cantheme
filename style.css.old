/** Styles for ol on single-post pages **/
div.single-post { counter-reset: my-badass-counter; }
div.single-post ol { list-style:none; }
div.single-post ol li:before {
	content: counter(my-badass-counter);
	counter-increment: my-badass-counter;
	margin-right:10px;
	font-size:2em;
	display:inline-block;
	margin-left: -1em;
}
